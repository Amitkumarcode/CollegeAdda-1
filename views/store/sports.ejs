	<script>
		var max_limit = 7
		$('#show-more').click(function () {
			move(2500)
			max_limit = max_limit + 7
			getProducts('SPORTS%20AND%20HOBBIES', max_limit)
		})
	</script>



	<script>
		if ("<%=user.picture%>" != "") {
			$('#login').hide();
			$('#profile').show();
			$('#wishlist').show();
		}
		else {
			$('#login').show();
			$('#profile').hide();
			$('#wishlist').hide();
		}


		$('#filter').click(function () {
			$('#aside').show()
		})

		if ($(document).width() < 700) {
			$('#filter').show()

			$('#apply-filter').click(function () {
				$('#aside').hide()
			})
			$('#cancel-filter').click(function () {
				$('#aside').hide()
			})

		}

	</script>

	<script>
		$(document).ready(function(){
			let college_name = getCookie('college')
			console.log(college_name)
			if(!college_name){
				$('#pop-coll-modal').click()
			}
			else{
				$('#search').val(college_name)
				$('#coll-data').text(college_name)
				getProducts('SPORTS%20AND%20HOBBIES', max_limit)
			}

			$.get('/products/count/SPORTS%20AND%20HOBBIES?college='+ $('#search').val() ,function (count) {
				$('#total-qt').html(count)
			})
		})


		$('#search-btn').click(function (e) {
			move(2500)
			$.get('/products/count/SPORTS%20AND%20HOBBIES?college='+ $('#search').val() ,function (count) {
				$('#total-qt').html(count)
			})
			$('#coll-data').text($('#search').val())
			setCookie('college',$('#search').val() , 10)
			e.preventDefault()
			getProducts('SPORTS%20AND%20HOBBIES', max_limit)
		})

		$('#apply-filter').click(function(){
			getProducts('SPORTS%20AND%20HOBBIES', max_limit, $('form').serialize())
		})

	</script>

