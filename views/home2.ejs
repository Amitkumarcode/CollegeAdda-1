        <script>
            $('#moreProduct').click(function (event) {
                event.preventDefault();
                <% no_of_item_displayed += 4; %>
                    <% var limit = (product.length < no_of_item_displayed) ? product.length : no_of_item_displayed; %>
                    $('.products').append(`<% for(var i=no_of_item_displayed-4; i< limit ;i++) {var prod_detail=product[i]; %>
                    <div class="card col-6 col-md-3 col-lg-3 col-sm-4"
                        style="background-color: inherit;display: inline-block;border: none; margin: 10px 0px;">
                        <img class="card-img-top" src="/image/<%=prod_detail.image[0]%>" alt="Product Image"
                            style="padding: 10px 30px;height: 170px;">
                        <div class="card-body" style="padding: 2px;">
                            <p class="card-text" stlye="font-size: large;font-weight: bolder;">₹ <%=prod_detail.price%>
                            </p>
                            <p class="card-text prod-title" style="font-size: small;"><%=prod_detail.title%>
                            </p>
                        </div>
                    </div>
                    <%}%>`);

                    $('.card').on('click', function () {
                const title = $(this).find('.prod-title').text();

                window.location.href = '/items/' + title;
            });
            });

            if ("<%=user.picture%>" != "") {
                $('.logged-in').hide();
                $('.avatar').show();
                $('.sell-button').addClass('mt-2');
            }
            $('.card').on('click', function () {
                const title = $(this).find('.prod-title').text();
                window.location.href = '/items/' + title;
            });


            $('#search').on('keyup', function (e) {
                if (e.keyCode == 39) {
                    $(".searched:hover").next().focus();

                }
                if (e.keyCode == 37) {
                    $(".searched:focus").prev().focus();

                }

                $('#searchResult').html(``);
                $('#searchResult').show();
                let searchData = $(this).val();
                let newReg = new RegExp(searchData, "i");
                $.getJSON('./State_City.json', function (data) {
                    data.forEach((clg_name) => {
                        if (clg_name.college.search(newReg) != -1) {
                            $('#searchResult').append(`<li class="list-group-item searched">${clg_name.college}</li>`);
                            $('.searched').click(function(){                                
                                window.location.href = 'https://college-adda.herokuapp.com/' + $(this).text();
                            });
                        }
                        if (searchData == '') {
                            $('#searchResult').html(``);
                            $('#searchResult').hide();
                        }
                    });
                });
            });
            $('#search').focusout(function (e) {
                setTimeout(()=>{
                    $('#searchResult').hide();
                },500);
            });
            $('#search').on('focus', function (e) {
                $('#searchResult').show();
            });

        </script>
</body>

